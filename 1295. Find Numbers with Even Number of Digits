
class Solution {
    public int findNumbers(int[] nums) {
        int count = 0;
        for (int i = 0; i < nums.length; i++) {
            int digit = 0;
            int temp = nums[i]; // Move this outside the loop
            while (temp > 0) {
                temp = temp / 10;
                digit += 1;
            }
            if (digit % 2 == 0) {
                count++;
            }
        }
        return count;
    }
}
//every time the temp is updated to new array element value
