public String largestGoodInteger(String num) {
    String maxNum = "";
    
    for (int i = 0; i <= num.length() - 3; i++) {
        if (num.charAt(i) == num.charAt(i + 1) && num.charAt(i + 1) == num.charAt(i + 2)) {
            String temp = num.substring(i, i + 3);
            if (temp.compareTo(maxNum) > 0) {
                maxNum = temp;
            }
        }
    }
    
    return maxNum;
}
